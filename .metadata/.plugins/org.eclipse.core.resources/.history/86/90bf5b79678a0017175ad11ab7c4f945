#include "stdio.h"
#include "math.h"
void main(){
	int da[3][3];
	int i ,j,temp;
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("enter the item (%d ,%d)",i,j);
			fflush(stdin);fflush(stdout);
			scanf("%d",da[i][j]);
		}
	}
}

