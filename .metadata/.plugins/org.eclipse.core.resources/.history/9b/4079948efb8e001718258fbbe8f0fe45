#include "stdio.h"
#include "math.h"

int calcmin(int a[],int n);

void main()
{
	int arr[]={1,5,8,6,-1,5,3,4};
	int s=sizeof(arr);
	calcmin(arr,s);
}

int calcmin(int a[],int n)
{
	int min=a[0];
	for(int i=1;i<=n;i++)
	{
		if(a[i]<min) min=a[i];

	}
	return min;
}


