#include "stdio.h"
void main(){

	float arr[10];
	for(int i=0;i<10;i++){
		printf("enter student %d degree :",i+1);
		fflush(stdin);fflush(stdout);
		scanf("%f",&arr[i]);
	}
	for(int i=0;i<10;i++){
		printf("student %d degree is :%f\n",i+1,arr[i]);
	}
}

